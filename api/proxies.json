{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
					"routetobase": {
						"matchCondition": {
							"route": "/",
							"methods": [
								"GET"
							]
						},
						"backendUri": "https://todolistyoanduboin.blob.core.windows.net/jshtml"
					},
					"jsproxy": {
						"matchCondition": {
							"route": "/js/{*restOfPath}",
							"methods": [
								"GET"
							]
						},
						"backendUri": "https://todolistyoanduboin.blob.core.windows.net/jshtml"
					}
    }
}
